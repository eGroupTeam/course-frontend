import React from 'react';

interface ProductProps {
  desc: string;
  price: number;
  stock: number;
}

const Product: React.FC<ProductProps> = ({ desc, price, stock }) => {
  return (
    <tr>
      <td>{desc}</td>
      <td>{price}</td>
      <td>{stock}</td>
    </tr>
  );
};

export default Product;

const createProduct = (product: ProductType) => {
  return (
    <Product
      key={product.desc}
      desc={product.desc}
      price={product.price}
      stock={product.stock}
    />
  );
};

